@model CondominiumCoOwnersSystem.Web.ViewModels.CompanyInfo.CompanyInfoViewModel


<section class="card text-center">
    <div class="card-header">
        <h3>@Model.CompanyTypeTypeOfServices</h3>
    </div>
    <div class="card-body">
        <div class="card-body" id="test">
            <div class="col-md-3 d-inline-block"><i class="fas fa-map-marker-alt fa-4x mb-3"></i> Адрес: @Model.Address</div>
            <div class="col-md-3 d-inline-block"><i class="fas fa-phone fa-4x mb-3"></i> Телефон: @Model.PhoneNumber</div>
            <div class="col-md-3 d-inline-block"><i class="fas fa-globe fa-4x mb-3"></i> Емейл: @Model.Email</div>
        </div>
    </div>
</section>